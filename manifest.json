{
  "name": "Notify Extension",
  "version": "1.0.0",
  "description": "Extension for notify",
  "manifest_version": 3,
  "author": "Arun Bohra",
  "action": {
    "default_title": "Notify Extension"
  },
  "content_scripts": [
